/* Confirm Dialog */


* {
    background-color:       rgba (47, 51, 64, 95%);
    text-color:             #FFFCFFFF;
	font:				 	"RobotoCondensed Bold 12";
}

window {
    width:      			225px;
    padding:    			25px;
    border:					1px;
    border-radius:          0px;
    border-color:           @border;
    location:               center;
    y-offset:               -2em;
}

entry {
    expand: 				true;
    text-color:				#FFFCFFFF;
}
